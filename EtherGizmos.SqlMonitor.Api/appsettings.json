{
  "Logging": {
    "LogLevel": {
      "Hangfire": "Debug",
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Connections": {
    "Database": {
      "Data Source": null,
      "Initial Catalog": null,
      "TrustServerCertificate": null,
      "Integrated Security": null,
      "Application Name": "SQL Monitor"
    },
    "Caching": {
      "Use": "Redis",
      "Redis": {
        "SslHost": null
      }
    },
    "MassTransit": {
      "Use": "RabbitMQ",
      "RabbitMQ": {
        "Host": null,
        "Username": null,
        "Password": null
      }
    }
  },
  "Jobs": {
    "EnqueueMonitorQueries": {
      "RunFrequency": "0/15 * * * * *",
      "QueryCheckFrequency": "0/1 * * * * *"
    }
  },
  "Caching": {
    "Keys": {
      "AllInstances": {
        "AbsoluteExpirationRelativeToNow": "0.00:05:00"
      },
      "AllQueries": {
        "AbsoluteExpirationRelativeToNow": "0.00:05:00"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {}
      }
    ]
  }
}
